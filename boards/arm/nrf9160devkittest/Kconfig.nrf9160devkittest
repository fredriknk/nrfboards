	select SOC_NRF9160_SICA if BOARD_NRF9160DEVKITTEST_NRF9160 || BOARD_NRF9160DEVKITTEST_NRF9160_NS
	select SOC_NRF52840_QIAA if BOARD_NRF9160DEVKITTEST_NRF52840
